<body id="index_home">
    <label>
      <h2 class"workout-text" style="color: #F3CAC3;">Workouts:</h2>
    </label>
    <ul>
      <% @workouts.each do |workout| %>
        <li class="workouts">
          <h5><a class="workouts"href="/workouts/<%= workout.id %>"><%= workout.work_out %></a></h5>
        </li>
      <% end %>
      <br>
      <% @user_workouts.each do |workout| %>
        <li class="workouts">
          <h5><a class="workouts"href="/workouts/<%= workout.id %>"><%= workout.work_out %></a></h5>
        </li>
      <% end %>
      <br>
      <a class="link-two" href="/workouts/new">Create New Workout</a>
    </ul>
    </body>
